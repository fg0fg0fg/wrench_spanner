<div class="container">
  <div class="row justify-content-center mt-3">
    <div class="col-md-6">
      <table class="table">
        <% @users.each do |user| %>
        <tbody>
          <tr>
            <td style="width: 20%;">
              <%= link_to admin_user_path(user) do %>
                <%= image_tag user.get_profile_image, size: "50x50", class: "d-block mx-auto", style: "border-radius: 50%;" %>
              <% end %>
            </td>
            <td style="width: 20%;"><%= user.name %></td>
            <td style="width: 20%;">プロフィール</td>
          </tr>
        </tbody>
        <% end %>
      </table>
    </div>
  </div>
</div>